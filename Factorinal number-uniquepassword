package HealthInsuranceIntegration;

public class FACTIONALnumber {

	public static void main(String[] args) {
		int charactor=6;
	long uniquepassword=	Factionalnumber(charactor);
	  System.out.println(uniquepassword);
	}

	private static long Factionalnumber(int charactor) {
		if(charactor==0) {
			return 1;
		}
		long result=1;
		for(int i=1;i<=charactor;i++) {
			result=result*i;
		}
		return result;
	}
		
	}
